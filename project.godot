"res://Assets/": "purple",
"res://Audio/": "red",
"res://PCs/": "blue",
"res://Resources/": "green",
"res://Scenes/": "yellow",
"res://Scripts/": "red",
"res://Worlds/": "teal",
"res://XeluController&KeyPrompts/": "pink"
}

[filesystem]

import/blender/enabled=false

[gui]

theme/custom="res://UI/new_theme.tres"

